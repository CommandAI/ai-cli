{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "aiHelloWorld",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/ai.js",
            "args": [
                "In a directory called hello_world create a nodejs application that says hello world. "
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "aiQUERY",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/aiq.js",
            "args": [
                "mysql show me all the records for transactions"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "aiQUERY delete",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/aiq.js",
            "args": [
                "mysql delete all the transaction records"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "aiConfig",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/ai.js",
            "args": [
                "config"
            ],
            "console": "integratedTerminal"
        }
    ]
}